{
  "name": "coding-agent",
  "version": "1.0.2",
  "description": "Coding agent for ideation",
  "main": "./src/index.js",
  "publisher": "jodonoghue",
  "engines": {
        "node": ">=12.10.0",
        "vscode": "^1.80.0"
      },
  "keywords": [],
  "author": "Jonathan O'Donoghue",
  "license": "ISC",
  "repository": "https://github.com/JODonogh/coding-agent",
  "activationEvents": [
    "onCommand:coding-agent.startExplanation",
    "onView:codingAgentView"
