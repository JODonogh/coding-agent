    "commands": [
      {
        "command": "coding-agent.startExplanation",
        "title": "Start Code Explanation"
      }
    ]
  },
  "dependencies": {
    "@langchain/community": "^0.3.46",
    "@langchain/langgraph": "^0.3.1",
    "@langchain/ollama": "^0.2.2",
    "@langchain/openai": "^0.5.13",
    "dotenv": "^16.5.0"
